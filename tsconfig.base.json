{
  "compileOnSave": false,
  "angularCompilerOptions": {
    "strictInjectionParameters": true,
    "strictTemplates": true
  },
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "noErrorTruncation": true,
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2019",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@orcha-user-auth-starter/server/core/services": [
        "libs/server/core/services/src/index.ts"
      ],
      "@orcha-user-auth-starter/server/core/domain": [
        "libs/server/core/domain/src/index.ts"
      ],
      "@orcha-user-auth-starter/server/orcha": [
        "libs/server/orcha/src/index.ts"
      ],
      "@orcha-user-auth-starter/client/shared/data-access": [
        "libs/client/shared/data-access/src/index.ts"
      ],
      "@orcha-user-auth-starter/client/shell": [
        "libs/client/shell/src/index.ts"
      ],
      "@orcha-user-auth-starter/shared/domain": [
        "libs/shared/domain/src/index.ts"
      ],
      "@orcha-user-auth-starter/shared/util": [
        "libs/shared/util/src/index.ts"
      ],
      "@orcha-user-auth-starter/client/shared/util": [
        "libs/client/shared/util/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}